pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                  git 'https://github.com/executeautomation/cucumberbasic.git'
            }
        }
        stage('clean') {
            steps {
              bat 'mvn clean' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
